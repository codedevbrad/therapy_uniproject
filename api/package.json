{
    "name": "node-js-getting-started",
    "version": "0.3.0",
    "description": "A sample Node.js app using Express 4",
    "engines": {
        "node": "12.x"
    },
    "main": "src/server.js",
    "scripts": {
        "server:dry": "node src/server.js",
        "server:watch": "nodemon bin/dev",
        "native-install": "npm install --prefix client",
        "native-run": "npm run start --prefix client",
        "app-run-all": "concurrently \"npm run server-watch\" \"npm run client-run\""
    },
    "dependencies": {
        "@codedevbrad/serverutils": "^1.0.2",
        "axios": "^0.20.0",
        "bcrypt": "^5.0.0",
        "concurrently": "^5.1.0",
        "cors": "^2.8.5",
        "express": "^4.15.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.5",
        "morgan": "^1.9.1",
        "twilio": "^3.49.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "dotenv": "^8.2.0",
        "jest": "^26.6.0",
        "mocha": "^7.1.1"
    },
    "keywords": [
        "node",
        "heroku",
        "express"
    ],
    "license": "MIT"
}
